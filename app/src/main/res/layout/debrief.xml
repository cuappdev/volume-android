<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/jdsfksa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        android:text="Share what you read"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/adead"
        android:layout_width="275dp"
        android:layout_height="275dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/jdsfksa" />

    <TextView
        android:id="@+id/pub_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Slope Media"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="@id/adead"
        app:layout_constraintTop_toBottomOf="@id/adead" />

    <TextView
        android:id="@+id/sdad"
        style="@style/lato_bold"
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:minLines="2"
        android:text="An Ode to LetterBoxd, the Most Underrated App"
        android:textSize="24dp"
        app:layout_constraintStart_toStartOf="@id/adead"
        app:layout_constraintTop_toBottomOf="@id/pub_name" />

    <TextView
        android:id="@+id/pop"
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:minLines="2"
        android:text="An Ode to LetterBoxd"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="@id/adead"
        app:layout_constraintTop_toBottomOf="@id/sdad" />

    <TextView
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:minLines="2"
        android:text="@string/dot"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@id/adead"
        app:layout_constraintStart_toStartOf="@id/adead"
        app:layout_constraintTop_toBottomOf="@id/pop" />


</androidx.constraintlayout.widget.ConstraintLayout>